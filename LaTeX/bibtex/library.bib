language = {eng},
month = {dec},
number = {4},
pages = {425--437},
pmid = {16382352},
title = {{Role of endogenous benzodiazepine ligands and their GABA-A--associated receptors  in hepatic encephalopathy.}},
volume = {20},
year = {2005}
}
