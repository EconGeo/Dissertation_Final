author = {Arribas-bel, Daniel and Nikamp, Peter and Scholten, Henk},
booktitle = {Analysis},
file = {::},
institution = {Vrije Universiteit},
keywords = {and computation,and geoda cen-,and urban,arizona state university,charles schmidt from the,darribas,department of economic analysis,es,europe,planning,school of geographical sciences,self-organizing maps,spain,ter for geospatial analysis,universidad de zaragoza,unizar,urban sprawl,usa},
