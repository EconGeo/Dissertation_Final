journal = {Ecological Economics},
keywords = {Constructivism,Critical realism,Ecological Economics,Ecological economics,Epistemology,Ideology,Interdisciplinarity,Logical empiricism,Methodology,Ontology,Post-normal science,Preanalytic vision,theory},
