number = {1},
pages = {77--89},
publisher = {Molecular Diversity Preservation International},
title = {{Sustainable globalisation}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0921800902002227},
volume = {44},
year = {2003}
}
