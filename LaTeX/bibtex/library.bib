month = {jul},
number = {3},
pages = {127--141},
title = {{Travel choices in pedestrian versus automobile oriented neighborhoods}},
url = {http://www.sciencedirect.com/science/article/pii/0967070X96000169 http://0-www.sciencedirect.com.catalog.library.colostate.edu/science/article/pii/0967070X96000169 http://0-www.sciencedirect.com.catalog.library.colostate.edu/science?{\_}ob=MiamiImageURL{\&}{\_}cid},
