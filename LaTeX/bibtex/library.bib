keywords = {Acetylcarnitine,Adult,Alanine Transaminase,Ammonia,Bilirubin,Cognition,Double-Blind Method,Electroencephalography,Female,Hepatic Encephalopathy,Humans,Intelligence Tests,Male,Middle Aged,Nootropic Agents,Prothrombin Time,Questionnaires,Serum Albumin,Severity of Illness Index,Treatment Outcome,blood,drug therapy,metabolism,physiology,psychology,therapeutic use},
language = {eng},
