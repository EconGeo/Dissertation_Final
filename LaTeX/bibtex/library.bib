file = {::},
keywords = {Quantitative Biology,Statistical Mechanics},
month = {jul},
pages = {20},
primaryClass = {cond-mat},
title = {{Epidemic outbreaks in complex heterogeneous networks}},
url = {http://arxiv.org/abs/cond-mat/0107267},
year = {2001}
}
