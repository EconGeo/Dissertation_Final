doi = {10.1109/ISGT.2012.6175772},
isbn = {978-1-4577-2159-5},
keywords = {Batteries,Battery Energy Storage Systems,Frequency Control,Power systems,Rate Variation,Renewable energy resources,Renewables,Smart Grid,Wind,Wind Energy,Wind energy generation,Wind power generation,battery energy storage,battery energy storage systems,battery storage plants,distributed renewable resources,droop control response,electric power distribution system,electricity infrastructure,energy storage,energy storage technology,frequency control,frequency regulation,grid management,innovation management,innovations,power grids,power system management,smart grid,wind energy,wind generation,wind power plants},
pages = {1--4},
title = {{Battery energy storage and wind energy integrated into the Smart Grid}},
