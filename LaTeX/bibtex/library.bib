pages = {175--186},
pmid = {1812391},
title = {{EEG and event related potentials in hepatic encephalopathy.}},
volume = {6},
year = {1991}
}
@article{Weissenborn2000,
abstract = {A patient suffering from liver cirrhosis presented with a bradykinetic-rigid syndrome suspected as Parkinson's disease. A detailed work-up of the case revealed hepatic encephalopathy as the cause of the neurological symptomatology. An alteration of striatal dopamine D2 receptor binding and dopamine re-uptake sites was demonstrated by 123I-iodobenzamide (IBZM) and 123I-beta-CIT single photon emission computed tomography (SPECT), respectively. It is suggested that the alteration of the dopamine re-uptake in cirrhotics may be the cause of an increased catabolism of dopamine in HE.},
