keywords = {Adult,Aged,Aged, 80 and over,Ammonia,Animals,Aspartic Acid,Female,Glutamates,Glutamic Acid,Hepatic Encephalopathy,Hippocampus,Humans,Liver Cirrhosis,Male,Middle Aged,Neurotransmitter Agents,Rats,Rats, Inbred Strains,blood,complications,drug effects,metabolism,pharmacokinetics,physiopathology,toxicity},
language = {eng},
month = {mar},
