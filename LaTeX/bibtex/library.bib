keywords = {Amino Acids, Branched-Chain,Anti-Bacterial Agents,Disaccharides,Evidence-Based Medicine,Gastrointestinal Agents,Hepatic Encephalopathy,Humans,Lactulose,Rifamycins,drug therapy,metabolism,therapeutic use},
language = {eng},
