journal = {American journal of clinical pathology},
keywords = {Acquired Immunodeficiency Syndrome,Acquired Immunodeficiency Syndrome: drug therapy,Anti-Retroviral Agents,Anti-Retroviral Agents: therapeutic use,CD4 Lymphocyte Count,CD4 Lymphocyte Count: instrumentation,CD4 Lymphocyte Count: methods,CD4 Lymphocyte Count: standards,Humans,Laboratories,Laboratories: standards,Laboratory Personnel,Laboratory Personnel: education,Laboratory Techniques and Procedures,Laboratory Techniques and Procedures: standards,Maintenance,Poverty,Quality Assurance, Health Care,Specimen Handling,Specimen Handling: methods},
month = {oct},
