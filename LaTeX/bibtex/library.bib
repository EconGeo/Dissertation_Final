title = {{Are fast food restaurants an environmental risk factor for obesity?}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1397859{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {3},
year = {2006}
}
