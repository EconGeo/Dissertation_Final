title = {{Microclimate modelling of street tree species effects within the varied urban morphology in the Mediterranean city of Tel Aviv, Israel}},
url = {http://dx.doi.org/10.1002/joc.1869 http://www3.interscience.wiley.com/cgi-bin/fulltext?ID=122255724{\&}PLACEBO=IE.pdf{\&}mode=pdf},
