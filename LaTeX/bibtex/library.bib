@misc{Hedegaard2012,
abstract = {Integrating large amounts of wind power in energy systems poses balancing challenges due to the variable and only partly predictable nature of wind. The challenges cover different time scales from intra-hour, intra-day to several days and seasonal level. Various flexibility measures are being discussed as candidates for contributing to large-scale wind power integration and these also differ in terms of the time scales at which they can operate. In this paper, using the case of West Denmark in 2025 with an expected 57 {\%} wind power penetration, wind power impacts on different time scales are analysed. Results show consecutive negative and high net load period lengths indicating a potential for flexible technologies capable of charging/activating demand and discharging/inactivating demand in periods of one hour to around half a day, providing intra-day balancing. The analysis also suggests a potential for flexible technologies responding to high/low net load periods of lengths approaching one day to several days, providing intra-week/s balancing. In addition, the results indicate a physical potential for seasonal balancing. In the study, battery electric vehicles, small heat pumps in individual hourses, large heat pumps and electric boilers in district heating grids, which are all considered promising technologies for wind power integration, are moreover categorized with respect to the time scales at which they will typically be suited for this purpose. While all of the technologies are found suited for providing intra-hour and intra-day balancing, only large heat pumps and electric boilers are assessed suited for providing balancing intra-week. If combined with large seasonal thermal storages, large heat pumps and electric boilers could also provide flexibility across months.},
author = {Hedegaard, Karsten and Meibom, Peter},
booktitle = {Renewable Energy},
doi = {http://dx.doi.org/10.1016/j.renene.2011.06.034},
file = {::},
issn = {0960-1481},
keywords = {Electricity storage,Net load,Power system balancing,Wind power},
month = {jan},
number = {1},
pages = {318--324},
title = {{Wind power impacts, electricity storage and heat measures - a time scale perspective}},
url = {http://www.sciencedirect.com/science/article/pii/S0960148111003594},
volume = {37},
year = {2011}
}
@article{Sioshansi2011,
abstract = {One economic disincentive to investing in wind generation is that the average market value of wind energy can be lower than that of other generation technologies. This is driven by the exercise of market power by other generators and the fact that the ability of these generators to exercise market power is inversely related to real-time wind availability. We examine the use of energy storage to mitigate this price suppression by shifting wind generation from periods with low prices to periods with higher prices. We show that storage can significantly increase the value of wind generation but the currently high capital cost of storage technologies cannot be justified on the basis of this use. Moreover, we demonstrate that this use of storage can reduce consumer surplus, the profits of other non-wind generators, and social welfare. We also examine the sensitivity of these effects to a number of parameters including storage size, storage efficiency, ownership structure, and market competitiveness--showing that a more-competitive market can make storage significantly more valuable to a wind generator. [ABSTRACT FROM AUTHOR]},
annote = {Accession Number: 67360414; Sioshansi, Ramteen 1; Email Address: sioshansi.1@osu.edu; Affiliation: 1: Integrated Systems Engineering Department, The Ohio State University, Columbus, Ohio, United States; Source Info: 2011, Vol. 32 Issue 2, p1; Subject Term: INVESTMENTS; Subject Term: SAVING {\&}amp; investment; Subject Term: FINANCE; Subject Term: WIND power; Subject Term: ENERGY storage; NAICS/Industry Codes: 523930 Investment Advice; NAICS/Industry Codes: 523999 Miscellaneous Financial Investment Activities; Number of Pages: 29p; Document Type: Article},
