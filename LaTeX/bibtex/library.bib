eprint = {PSCE.2011.5772524},
isbn = {9781612847870},
keywords = {batteries,distribution,energy storage,grid operation,power systems,solar power,transmission,wind power},
pages = {1--10},
primaryClass = {10.1109},
title = {{Value propositions for utility-scale energy storage}},
year = {2011}
}
