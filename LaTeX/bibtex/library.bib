keywords = {Ammonia,Animals,Cerebellum,Cognition,Disease Models, Animal,Hepatic Encephalopathy,Humans,Hyperammonemia,Male,Models, Animal,Nitro Compounds,Phosphorylation,Portacaval Shunt, Surgical,Protein Processing, Post-Translational,Proteomics,Rats,Rats, Wistar,Signal Transduction,Tyrosine,adverse effects,complications,drug effects,etiology,metabolism,methods,pathology},
