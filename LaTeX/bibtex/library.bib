@article{Montagnese2012,
abstract = {Awareness of previous hepatic encephalopathy (HE) and compliance with treatment can probably reduce HE recurrence. The aim of this study was to assess the degree of awareness of previous HE and its treatment in a group of cirrhotic patients and their caregivers. Thirty-five cirrhotic patients with a history of HE and their caregivers (n = 31) were enrolled. Patients underwent evaluation of HE (clinical, psychometry and electroencephalography), quality of life (SF36 questionnaire), and awareness of HE/treatment on an ad hoc questionnaire (QAE). Caregivers underwent the QAE plus the Caregiver Burden Inventory. On the day of study, 7 patients were unimpaired, 8 had minimal and 20 low-grade overt HE. Of the patients, 37 {\%} were aware of previous HE, 6 {\%} of being on treatment and 6 {\%} understood treatment effects. Of the caregivers, 48 {\%} were aware of previous HE, 6 {\%} of their relative being on treatment and 6 {\%} understood treatment effects. Significant correlations were observed between neuropsychiatric status/linear HE indices and both the patients' quality of life and the caregivers' burden. In conclusion, HE awareness was poor in both patients and caregivers, most likely in relation to insufficient/inadequate provision of information.},
author = {Montagnese, S and Amato, E and Schiff, S and Facchini, S and Angeli, P and Gatta, A and Amodio, P},
doi = {10.1007/s11011-012-9325-7},
issn = {1573-7365 (Electronic)},
journal = {Metabolic brain disease},
keywords = {Aged,Caregivers,Cost of Illness,Educational Status,Electroencephalography,Female,Health Knowledge, Attitudes, Practice,Hepatic Encephalopathy,Humans,Liver Cirrhosis,Male,Mental Disorders,Middle Aged,Neuropsychological Tests,Psychometrics,Quality of Life,Questionnaires,complications,etiology,psychology,therapy},
language = {eng},
month = {dec},
number = {4},
pages = {567--572},
pmid = {22791269},
title = {{A patients' and caregivers' perspective on hepatic encephalopathy.}},
volume = {27},
year = {2012}
}
@article{Barrat2004a,
abstract = {We present a general model for the growth of weighted networks in which the structural growth is coupled with the edges' weight dynamical evolution. The model is based on a simple weight-driven dynamics and a weights' reinforcement mechanism coupled to the local network growth. That coupling can be generalized in order to include the effect of additional randomness and non-linearities which can be present in real-world networks. The model generates weighted graphs exhibiting the statistical properties observed in several real-world systems. In particular, the model yields a non-trivial time evolution of vertices properties and scale-free behavior with exponents depending on the microscopic parameters characterizing the coupling rules. Very interestingly, the generated graphs spontaneously achieve a complex hierarchical architecture characterized by clustering and connectivity correlations varying as a function of the vertices' degree.},
archivePrefix = {arXiv},
arxivId = {cond-mat/0406238},
author = {Barrat, Alain and Barthelemy, Marc and Vespignani, Alessandro},
doi = {10.1103/PhysRevE.70.066149},
eprint = {0406238},
file = {::},
keywords = {Statistical Mechanics},
month = {jun},
pages = {1--13},
primaryClass = {cond-mat},
title = {{Modeling the evolution of weighted networks}},
url = {http://arxiv.org/abs/cond-mat/0406238},
year = {2004}
}
