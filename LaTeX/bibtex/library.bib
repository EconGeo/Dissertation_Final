journal = {European Wind Energy Conference},
keywords = {July 2006,NREL/CP-500-39955,grid,grid integration,utilities,utility,variability,wind energy,wind energy cost analysis,wind generation,wind power variability,wind turbines},
pages = {1--16},
title = {{Grid Impacts of Wind Power Variability: Recent Assessments from a Variety of Utilities in the United States}},
year = {2006}
}
